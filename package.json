{
  "name": "inflation-project",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "pnpm -C apps/web dev",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "prisma:migrate": "pnpm -C apps/web prisma migrate dev",
    "prisma:studio": "pnpm -C apps/web prisma studio"
  }
}
